<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="osha.campaigntoolkit">
<body>

<metal:content-core fill-slot="content-core">
    <metal:content-core define-macro="content-core">
       	<a class="link-parent" tal:attributes="href string:${context/aq_inner/aq_parent/absolute_url}">Back to Overview</a>
        <h3 i18n:translate="heading_organisation">Organisation:</h3>
       	<span tal:content="context/organisation"></span>
	<h3 i18n:translate="heading_country">Country:</h3>
       	<span tal:content="context/country"></span>
        <h3 i18n:translate="heading_link">Link:</h3>
       	<a tal:attributes="href context/link"
	   tal:content="context/link"></a>
    </metal:content-core>
</metal:content-core>

</body>
</html>